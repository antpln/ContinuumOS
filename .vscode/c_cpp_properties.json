{
    "configurations": [
        {
            "name": "i686-elf",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include"
            ],
            "defines": [
                "__i386__",
                "__kernel__"
            ],
            "compilerPath": "/usr/bin/i686-elf-gcc",
            "cStandard": "gnu99",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x86",
            "compilerArgs": [
                "-ffreestanding",
                "-O2",
                "-Wall",
                "-Wextra"
            ]
        }
    ],
    "version": 4
}
